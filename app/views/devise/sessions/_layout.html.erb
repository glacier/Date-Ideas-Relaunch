<!-- <h2>Log In</h2> -->
<div class="registration_container modal_container span-14 last">
	<div id="login_form">
		<%= generate_ribbon_header :h3, :"Log In", 8 %>
		<%= render :partial => 'login_form' %>
	</div>
	<div id="signup">
		<p>Need an account?</p>
			<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
			  <%= link_to "Sign up", new_registration_path(resource_name), :remote => true, :class=>"di_button" %>
			<% end -%>
	</div>
</div>
