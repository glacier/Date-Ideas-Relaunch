<%= content_for :header do %>
	<div id="logo">
		<%= link_to root_path do %> 
			<%=image_tag "di_logo_white.png", :width => 90, :height => 60 %>
		<% end %>
	</div>
<% end %>

<%= content_for :content do %>
	<div id="main" class="span-14 append-1">
		<%= yield %>
	</div>
	
	<div id="sidebar_right" class="span-9 last">
			<%= yield :sidebar_right %>
	</div>
<% end %>

<%= render :file => 'layouts/application' %>